{
  "mountDir": "../MOCKS",
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "3000",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "pathEncryption":{
    "keySize": 24,
    "ivSize": 16,
    "encryptionPass": "PATH_PASS",
    "salt":"PATH_SALT",
    "iv": "PATH_IV",
    "algorithm": "aes-192-cbc",
    "outputType": "base64"

  }
}
